@model ConsultaARSHospital.Especialidades
@{
    ViewBag.Title = "Eliminar Especialidad";
}

<h2>Eliminar Hospital</h2>

<div>
    <h4>¿Estás seguro de que quieres eliminar este especialidad?</h4>
    <hr />

    <dl class="dl-horizontal">
        <dt>Nombre:</dt>
        <dd>@Html.DisplayFor(model => model.nombre_especialidad)</dd>

        <dt>Apellido:</dt>
        <dd>@Html.DisplayFor(model => model.descripcion)</dd>

       <dt>ID Hospital:</dt>
        <dd>@Html.DisplayFor(model => model.id_doctor)</dd>
    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Borrar" class="btn btn-default" /> |
            @Html.ActionLink("Volver", "Mision")
        </div>
    }
</div>